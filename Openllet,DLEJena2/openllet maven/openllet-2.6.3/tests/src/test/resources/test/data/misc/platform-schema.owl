    <!-- http://www.inmindcomputing.com/platform/platform-schema.owl#BusinessType -->

    <owl:Class rdf:about="&ps;BusinessType">
        <rdfs:label rdf:datatype="&xsd;string">BusinessType</rdfs:label>
